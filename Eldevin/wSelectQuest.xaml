<Window x:Class="Eldevin.wSelectQuest"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Eldevin"
        mc:Ignorable="d"
        Title="wSelectQuest" Height="850" Width="1060">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
            <RowDefinition Height="40"></RowDefinition>
        </Grid.RowDefinitions>
        <Label Style="{StaticResource Label}" Content="Select quest"></Label>
        
        <WrapPanel Grid.Row="1" Name="wrpSkillQuest">
        </WrapPanel>

        <StackPanel Grid.Row="2" Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Center">
            <Button Name="btnOK" Content="OK" Style="{StaticResource Button}"></Button>
            <Button Name="btnCancel" Content="Cancel" Style="{StaticResource Button}"></Button>
        </StackPanel>

    </Grid>
</Window>
